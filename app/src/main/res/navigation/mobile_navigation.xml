<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    app:startDestination="@+id/nav_home">

    <fragment
        android:id="@+id/nav_home"
        android:name="com.elab.dsdr.ui.home.HomeFragment"
        android:label="@string/menu_home"
        tools:layout="@layout/fragment_home">
        <action
            android:id="@+id/action_nav_home_to_doctors"
            app:destination="@id/doctors" />
        <action
            android:id="@+id/action_nav_home_to_ambulance4"
            app:destination="@id/ambulance4" />
        <action
            android:id="@+id/action_nav_home_to_dental"
            app:destination="@id/dental" />
        <action
            android:id="@+id/action_nav_home_to_aboutUs"
            app:destination="@id/aboutUs" />
        <action
            android:id="@+id/action_nav_home_to_zillaChamber"
            app:destination="@id/zillaChamber" />
        <action
            android:id="@+id/action_nav_home_to_healthTipsMain"
            app:destination="@id/healthTipsMain" />
    </fragment>

    <fragment
        android:id="@+id/hotline_emergency"
        android:name="com.elab.dsdr.ui.emergencyHotline.HotlineMain"
        android:label="@string/hotline_emergency"
        tools:layout="@layout/fragment_hot_main" />

    <fragment
        android:id="@+id/doctors"
        android:name="com.elab.dsdr.ui.doctorsHelpline.doctorHelpline"
        android:label="fragment_doctors"
        tools:layout="@layout/fragment_doctors" />

    <fragment
        android:id="@+id/ambulance4"
        android:name="com.elab.dsdr.ui.ambulance.Ambulance"
        android:label="fragment_ambulance"
        tools:layout="@layout/fragment_ambulance" />

    <fragment
        android:id="@+id/dental"
        android:name="com.elab.dsdr.ui.dentalHelpline.Dental"
        android:label="fragment_dental"
        tools:layout="@layout/fragment_dental" />
    <fragment
        android:id="@+id/aboutUs"
        android:name="com.elab.dsdr.aboutUs"
        android:label="fragment_about_us"
        tools:layout="@layout/fragment_about_us" />
    <fragment
        android:id="@+id/zillaChamber"
        android:name="com.elab.dsdr.ui.zillaChambers.zillaChamber"
        android:label="fragment_zilla_chamber"
        tools:layout="@layout/fragment_zilla_chamber" />
    <fragment
        android:id="@+id/healthTipsMain"
        android:name="com.elab.dsdr.ui.healthTips.healthTipsMain"
        android:label="fragment_health_tips_main"
        tools:layout="@layout/fragment_health_tips_main" />

</navigation>